{
	"compilerOptions": {
		"target": "es2020",
		"module": "CommonJS",
		"lib": ["es2020"],
		"allowJs": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"strict": false,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "build", "scripts", "webpack", "jest"],
	}
	